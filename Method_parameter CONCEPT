// Define a class named Robot
class Robot {
    // Method to make the robot speak, taking a String parameter 'text'
    public void speak(String text) {
        System.out.println(text);
    }

    // Method to make the robot jump, taking a double parameter 'height'
    public void jump(double height) {
        System.out.println("Jumping " + height);
    }
}

// Main class named RobotApp
public class RobotApp {

    // Main method, the entry point of the Java program
    public static void main(String[] args) {
        // Create an instance of the Robot class
        Robot robot = new Robot();

        // Call the 'speak' method of the Robot class and pass "Hi im Poorvik" as an argument
        robot.speak("Hi im Poorvik");

        // Call the 'jump' method of the Robot class and pass 5.80 as an argument
        robot.jump(5.80);

        // Create a String variable 'greeting' with the value "Hello dear :"
        String greeting = "Hello dear :";

        // Call the 'speak' method of the Robot class and pass 'greeting' as an argument
        robot.speak(greeting);
    }
}
