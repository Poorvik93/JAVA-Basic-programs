class MyClass {
    private int id;
    private String name;

    public MyClass(int id, String name) {
        this.id = id;
        this.name = name;
    }

    @Override
    public String toString() {
        return "MyClass{id=" + id + ", name='" + name + "'}";
    }
}

public class ToStringExample {
    public static void main(String[] args) {
        MyClass myObj = new MyClass(1, "John Doe");

        // Implicitly calls myObj.toString() to obtain the string representation
        System.out.println("Object: " + myObj);
    }
}
